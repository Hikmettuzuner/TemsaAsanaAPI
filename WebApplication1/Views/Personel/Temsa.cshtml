@model WebApplication1.Models.Personel
@{
    ViewBag.Title = "Temsa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="body">
                @using (Html.BeginForm("Temsa", "Personel", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {         
                    <label for="password">Asana ID</label>
                    <div class="form-group">
                        <div class="form-line">
                            @Html.EditorFor(model => model.AsanaId, new { htmlAttributes = new { @class = "form-control", @maxlength = "150", @required = "required" } })
                        </div>
                    </div>

                    <br>
                }
            </div>
        </div>
    </div>
</div>
